<Window x:Class="Projekt.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Height="150" Width="150"/>
            <Button Content="Button" HorizontalAlignment="Left" Margin="525,150,0,0" VerticalAlignment="Top" Height="150" Width="150" Click="Button_Click"/>
        </StackPanel>
        <Frame x:Name="myFrame" Margin="0"/>
    </Grid>
</Window>
